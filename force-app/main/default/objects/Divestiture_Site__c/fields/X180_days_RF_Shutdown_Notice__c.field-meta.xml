<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>X180_days_RF_Shutdown_Notice__c</fullName>
    <externalId>false</externalId>
    <formula>IF(  TODAY()  &gt; Forecasted_Logical_Decommission_Date__c -180, 
(Forecasted_Logical_Decommission_Date__c  - 180), 
NUll)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Date that is 180 days before the  RF Shutdown Date Notice. This is blank once the 180 day lock period has been reached..</inlineHelpText>
    <label>180 Days Before RF Shutdown</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Date</type>
</CustomField>
